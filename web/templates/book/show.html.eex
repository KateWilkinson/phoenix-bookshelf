<h2>Show book</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @title %>
  </li>

  <li>
    <strong>Author</strong>
    <%= @author %>
  </li>

  <li>
    <strong>Isbn:</strong>
    <%= @book.isbn %>
  </li>

  <li>
    <strong>Publisher</strong>
    <%= @publisher %>
  </li>

</ul>

<%= link "Edit", to: book_path(@conn, :edit, @book) %>
<%= link "Back", to: book_path(@conn, :index) %>
